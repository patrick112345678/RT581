# PURPOSE: Makefile for the dump converter (.dump -> .pcap).

all:
	gcc -o dump_converter -DZB_PLATFORM_LINUX_PC32 -I. -I../../osif/include -I../../include -I../../include/osif dump_converter.c -lpcap
	gcc -o dump_parse dump_parse.c
	gcc -o hex_to_bin hex_to_bin.c

clean:
	rm -f *.o *.map dump_converter dump_parse hex_to_bin
