#  PURPOSE: Options file for tests list/sources generation using make
# */

LIBS =
LIBS_ED =
LIBS_ZGPD =
LIBS_MAC_CERT =

LINK_BINARY = @echo -n "ZC " ; x=`pwd`; x=`basename $${x}` ; echo $$x"_"$@ ../$$x/$(subst .o,.c,$^)

LINK_BINARY_ED = @echo  -n "ED " ; x=`pwd`; x=`basename $${x}` ; echo $$x"_"$@ ../$$x/$(subst .ed.o,.c,$^)

LINK_BINARY_ZGPD = @echo -n "ZGPD " ; x=`pwd`; x=`basename $${x}` ; echo $$x"_"$@ ../$$x/$(subst .zgpd.o,.c,$^)

LINK_BINARY_MAC = @echo -n "MAC " ; x=`pwd`; x=`basename $${x}` ; echo $$x"_"$@ ../$$x/$(subst .mac.o,.c,$^)


.SUFFIXES: .c .o .ed.o .zgpd.o .mac.o

.c.o:
	@touch $@

.c.ed.o:
	@touch $@

.c.zgpd.o:
	@touch $@

.c.mac.o:
	@touch $@

all: clean

pwd:
	@echo -n "TEST " ; basename `pwd`

clean: doclean

doclean:
	@rm -f *.o $(BINS)
