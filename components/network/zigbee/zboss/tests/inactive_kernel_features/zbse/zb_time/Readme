Test Time cluster support and it functionality.

================================================================================
run_md.sh:
================================================================================
    esi_device:
        #define ESI_DEV_ENDPOINT 1
        #define ESI_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_MASTER) | (1 << ZB_ZCL_TIME_MASTER_ZONE_DST) | (1 << ZB_ZCL_TIME_SUPERSEDING))
        g_dev_ctx.time_attrs.time = zb_get_utc_time();  /* We assume: that this time is trusted */
    esi_zr:
        #define ESI_DEV_ENDPOINT 5
        #define ESI_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_MASTER) | (1 << ZB_ZCL_TIME_MASTER_ZONE_DST) | (1 << ZB_ZCL_TIME_SUPERSEDING))
        write attributes:
            g_dev_ctx.time_attrs.time = zb_get_utc_time() + 3600;  /* We assume: that this time is trusted */
            zb_uint8_t time_status = ((1 << ZB_ZCL_TIME_MASTER) | (1 << ZB_ZCL_TIME_SYNCHRONIZED));
        SET_REAL_TIME_CLOCK CB isn't set.
        zb_uint8_t time_status = ((1 << ZB_ZCL_TIME_MASTER) | (1 << ZB_ZCL_TIME_SYNCHRONIZED));
    gas_device:
        #define MLC_REMOTE_COM_DEV_ENDPOINT 7
        #define GAS_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_SYNCHRONIZED))

        set real time clock cb isn't set.

================================================================================
run_md1.sh:
================================================================================
esi_device:
        #define ESI_DEV_ENDPOINT 1
        #define ESI_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_MASTER) | (1 << ZB_ZCL_TIME_MASTER_ZONE_DST) | (1 << ZB_ZCL_TIME_SUPERSEDING))
        g_dev_ctx.time_attrs.time = zb_get_utc_time();  /* We assume: that this time is trusted */
    esi_zr:
        #define ESI_DEV_ENDPOINT 5
        #define ESI_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_SYNCHRONIZED))
        write attributes:
            g_dev_ctx.time_attrs.time = zb_get_utc_time() + 3600;  /* We assume: that this time is trusted */
            zb_uint8_t time_status = ((1 << ZB_ZCL_TIME_SUPERSEDING));
        SET_REAL_TIME_CLOCK CB isn't set.
        zb_uint8_t time_status = ((1 << ZB_ZCL_TIME_SUPERSEDING));
    gas_device:
        #define MLC_REMOTE_COM_DEV_ENDPOINT 7
        #define GAS_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_MASTER) | (1 << ZB_ZCL_TIME_SYNCHRONIZED))

        set real time clock cb isn't set

================================================================================
run_md2.sh:
================================================================================
esi_device:
        #define ESI_DEV_ENDPOINT 1
        #define ESI_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_MASTER) | (1 << ZB_ZCL_TIME_SUPERSEDING))
        g_dev_ctx.time_attrs.time = ZB_ZCL_TIME_TIME_INVALID_VALUE;
    esi_zr:
        #define ESI_DEV_ENDPOINT 5
        #define ESI_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_MASTER_ZONE_DST))
        write attributes:
            g_dev_ctx.time_attrs.time = zb_get_utc_time() + 3600;  /* We assume: that this time is trusted */
            zb_uint8_t time_status = ((1 << ZB_ZCL_TIME_MASTER) | (1 << ZB_ZCL_TIME_SYNCHRONIZED));
        SET_REAL_TIME_CLOCK CB isn't set.
    gas_device:
        #define MLC_REMOTE_COM_DEV_ENDPOINT 7
        #define GAS_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_SYNCHRONIZED))

        ZB_ZCL_TIME_SET_REAL_TIME_CLOCK_CB(gas_set_real_time_clock_cb);

================================================================================
run_md3.sh:
================================================================================
esi_device:
        #define ESI_DEV_ENDPOINT 1
        #define ESI_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_SUPERSEDING))
        g_dev_ctx.time_attrs.time = ZB_ZCL_TIME_TIME_INVALID_VALUE;
    esi_zr:
        #define ESI_DEV_ENDPOINT 5
        #define ESI_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_SUPERSEDING))
        write attributes:
            g_dev_ctx.time_attrs.time = ZB_ZCL_TIME_TIME_INVALID_VALUE;
            zb_uint8_t time_status = ((1 << ZB_ZCL_TIME_MASTER));
        SET_REAL_TIME_CLOCK CB isn't set.
        g_dev_ctx.time_attrs.time = ZB_ZCL_TIME_TIME_INVALID_VALUE;
    gas_device:
        #define MLC_REMOTE_COM_DEV_ENDPOINT 7
        #define GAS_TIME_STATUS_INITIAL ((1 << ZB_ZCL_TIME_MASTER))

        set real time clock cb isn't set